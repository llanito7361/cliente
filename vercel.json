{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",  
        "use": "@vercel/node"
      },
      {
        "src": "cliente/package.json", 
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/index.js" },  
      { "src": "/(.*)", "dest": "/cliente/$1" }  
    ],
    "functions": {
      "api/index.js": {
        "runtime": "nodejs18.x"  
      }
    }
  }
  